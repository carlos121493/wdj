      </view>
      <view>
        <text>餐后优惠</text>
        <text>可获得100元代金卷</text>
      </view>-->
        <view bindtap="faslid">
          <text>备注</text>
          <text class="invoice"><text wx:if="{{order.remark!='undefined'}}">{{order.remark}}</text></text>
        </view>
      </view>
      <view class="ceshi"></view>
    </scroll-view>
    <view class="order-ac">
      <view class="go-back" bindtap="goback">返回</view>
      <view>
        <text class="pice">￥{{order.amount}}元</text>
        <form report-submit="true" bindsubmit="submits">
          <button class="determine {{color}}ba_color" formType="submit">确认</button>
        </form>
      </view>
    </view>
  </view>

  <!--发票信息-->
  <view class="facontainer {{faslid ? 'slidleft' : ''}}">
