<view class="container">
<view class="loading" wx:if="{{loading}}">
  <image src="../../images/hope.gif"></image>
</view>
  <view class="dheader">
    <navigator url="../shop/hdsm">
      <image wx:if="{{menuInfo.saleImg}}" src="{{api+menuInfo.saleImg}}"></image>
      <image wx:else src="{{api}}res/xcx/images/deflt_banner.png"></image>
    </navigator>
    <view calss="header-info">{{menuInfo.saleInfo}}</view>
  </view>
<!--横向-->
  <view class="page-body" wx:if="{{menutab==2}}">
    <view wx:if="{{tab_config.fixed}}" class="tab-bar {{color}}tab-bar">
      <view wx:for="{{tabs}}" wx:key="unique" data-index="{{index}}" bindtap="handlerTabTap" class="tab {{tab_config.active_tab==index?'{{color}}text_color':''}} " style="width: {{tab_config.item_width}}px;">
        <text>{{item.title}}</text>
      </view>
      <view style="width: {{tab_config.item_width}}px; left: {{tab_config.underline.offset}}px;" class="under-line withAnimate"></view>
    </view>
    <scroll-view wx:else class="tab-bar {{color}}tab-bar" scroll-x="true" bindscroll="onScroll" style="width: 100%;" scroll-left="{{tab_config.tab_left}}">
      <view wx:for="{{tabs}}" wx:key="unique" data-index="{{index}}" bindtap="handlerTabTap" class="tab {{tab_config.active_tab==index?'{{color}}ctext_color':''}} " style="width: {{tab_config.item_width}}px;">
        <text>{{item.title}}</text>
      </view>
      <view style="width: {{tab_config.item_width}}px; left: {{tab_config.underline.offset}}px;" class="under-line withAnimate"></view>
    </scroll-view>
    <view style="height:790rpx;background:#f2f2f2">
      <swiper class="swiper" indicator-dots="{{swipe_config.indicator_dots}}" autoplay="{{swipe_config.autoplay}}" interval="{{swipe_config.interval}}" duration="{{swipe_config.duration}}" current="{{swipe_config.current}}" bindchange="swiperChange">
          <block wx:key="unique" wx:if="{{menu.top>0}}">
          <swiper-item>
            <scroll-view scroll-y="true" style="height:790rpx;">
            <scroll-view scroll-x="true" class="tinfo1" wx:if="{{couponList.length>0}}">
              <view class="list {{color}}list" wx:for="{{couponList}}" bindtap="get" wx:key="unique">
                <view class="container">
                  <text class="tit">{{item.type==1?'满减券':'现金券'}}</text>
                  <view class="act" wx:if="{{item.full}}">满{{item.full}}<text style="font-size:24rpx;text-align:center">减{{item.value}}</text></view>
                  <view class="act" wx:else>{{item.value}}元</view>
                  <view class="bottom">立即领取</view>
                </view>
              </view>
            </scroll-view>
              <view class="swiper-item">
                <view class="list" wx:for="{{tjList}}" wx:key="unique">
                  <view class="img" bindtap="tcshow" data-g="{{item}}">
                    <text wx:if="{{item.isNew}}" class="new"></text>
                    <image src="{{api+item.logo}}"></image>
                  </view>
                  <view class="info">
                    <view class="name"><text>{{item.title}}</text><text wx:if="{{item.sns}}" class="yj {{color}}ctext_color" data-g="{{item}}" bindtap="tcshow">多规格</text></view>
                    <view class="num">
                      <text class="time" wx:if="{{item.cookType!=''}}">[{{item.cookType}}] <text wx:if="{{item.cookMinute>0}}">{{item.cookMinute}}分钟</text></text>
                      <text class="number" wx:if="{{item.orderCnt>0}}">{{item.orderCnt}}点过</text>
                    </view>
                    <view class="price">
                      <text>￥{{item.nowPrice}}</text>
                      <view class="calculate">
                        <view class="cnums {{newmap[item.id].NO>0?'show':''}}">
                          <view class="addCart" data-g="{{item}}" data-index="{{index}}" bindtap="{{item.sns?'tcshow':'tapDecCart'}}">
                            <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                          </view>
                        </view>
                        <text class="cnums nums {{newmap[item.id].NO>0?'show':''}}">{{newmap[item.id].NO}}</text>
                        <view class="addcart" wx:if="{{item.total>0}}" data-g="{{item}}" bindtap="{{item.sns?'tcshow':'tapAddCart'}}" data-index="{{index}}">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                        </view>
                        <view wx:if="{{item.total<=0}}" class="addcart">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
                        </view>
                      </view>
                    </view>
                  </view>
                </view>
                </view>     
            </scroll-view>
          </swiper-item>          
        </block>  

        <block wx:for="{{menu.cates}}" wx:key="unique" wx:for-item="cates">
          <swiper-item>
          <scroll-view scroll-x="true" class="tinfo1" wx:if="{{couponList.length>0&&menu.top==0&&index==0}}">
              <view class="list {{color}}list" wx:for="{{couponList}}" wx:key="unique" bindtap="get">
                <view class="container">
                  <text class="tit">{{item.type==1?'满减券':'现金券'}}</text>
                  <view class="act" wx:if="{{item.full}}">满{{item.full}}<text style="font-size:24rpx;text-align:center">减{{item.value}}</text></view>
                  <view class="act" wx:else>{{item.value}}元</view>
                  <view class="bottom" >立即领取</view>
                </view>
              </view>
            </scroll-view>
            <scroll-view scroll-y="true" style="height:790rpx;">
              <view class="swiper-item">
                <view class="list" wx:for="{{cates.glist}}" wx:key="unique">
                  <view class="img" bindtap="tcshow" data-g="{{item}}">
                    <text wx:if="{{item.isNew}}" class="new"></text>
                    <image src="{{api+item.logo}}"></image>
                  </view>
                  <view class="info">
                    <view class="name"><text>{{item.title}}</text><text wx:if="{{item.sns}}" class="yj {{color}}ctext_color" data-g="{{item}}" bindtap="tcshow">多规格</text></view>
                    <view class="num">
                      <text class="time" wx:if="{{item.cookType!=''}}">[{{item.cookType}}] <text wx:if="{{item.cookMinute>0}}">{{item.cookMinute}}分钟</text></text>
                      <text class="number" wx:if="{{item.orderCnt>0}}">{{item.orderCnt}}点过</text>
                    </view>
                    <view class="price">
                      <text>￥{{item.nowPrice}}</text>
                      <view class="calculate">
                        <view class="cnums {{newmap[item.id].NO>0?'show':''}}">
                          <view class="addCart" data-g="{{item}}" data-index="{{index}}" bindtap="{{item.sns?'tcshow':'tapDecCart'}}">
                            <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                          </view>
                        </view>
                        <text class="cnums nums {{newmap[item.id].NO>0?'show':''}}">{{newmap[item.id].NO}}</text>
                        <view class="addcart" wx:if="{{item.total>0}}" data-g="{{item}}" bindtap="{{item.sns?'tcshow':'tapAddCart'}}" data-index="{{index}}">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                        </view>
                        <view wx:if="{{item.total<=0}}" class="addcart">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
                        </view>
                      </view>
                    </view>
                  </view>
                </view>
              </view>
            </scroll-view>
          </swiper-item>
        </block>
      </swiper>
    </view>    
  </view>
<!--弹出详情-->
  <view class="tcontent {{tcshow?'':'tcshow'}}">
      <view class="nr">
      <!-- <swiper indicator-dots="{{indicatorDots}}"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <block wx:for="{{tcimgs}}">
          <swiper-item>
            <image src="{{api+item}}"/>
          </swiper-item>
        </block>
      </swiper> -->
         <swiper wx:if="{{tcimgs!=''}}" indicator-dots="{{indicatorDots}}" vertical="{{vertical}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" class="banner">
          <block wx:for="{{tcimgs}}" wx:key="unique">
            <swiper-item>
              <image class="tcimgs" src="{{api+item}}" />
            </swiper-item>
          </block>
        </swiper> 
        <view class="logo" wx:if="{{tcimgs==''}}">
            <image src="{{api+tcinfo.logo}}"></image>
        </view>
        <view class="content">
          <view class="name">{{tcinfo.title}}
            <text wx:if="{{tcinfo.hasXc}}" class="i-xc"></text>
            <text wx:for="{{tcinfo.spiceLevel}}" class="i-la" wx:key="unique"></text>
            <text wx:if="{{tcinfo.hasSuan}}" class="i-suan"></text>
          </view>
          <view class="price">￥{{tcinfo.nowPrice}}<text wx:if="{{tcinfo.oldPrice}}"> ￥{{tcinfo.oldPrice}}</text></view>
          <view class="remark" wx:if="{{tcinfo.remark}}">{{tcinfo.remark}}</view>
          <view class="num">
            <text class="time" wx:if="{{tcinfo.cookType!=''}}">[{{tcinfo.cookType}}]</text> <text wx:if="{{item.cookMinute>0}}">{{item.cookMinute}}分钟</text>
            <text class="number" wx:if="{{tcinfo.orderCnt>0}}">{{tcinfo.orderCnt}}点过</text>
          </view>
          <view class="foodspecs" wx:for="{{tcinfo.sns}}" wx:for-item="sns" wx:key="unique">
            <view class="tnum">
              <view class="sdec {{newmap[tcinfo.id+'_'+sns.id].NO>0?'show':''}}" data-tcinfo="{{tcinfo}}" data-index="{{index}}" bindtap="tapDecCart">
                <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
              </view>
              <text class="snum {{newmap[tcinfo.id+'_'+sns.id].NO>0?'show':''}}">{{newmap[tcinfo.id+'_'+sns.id].NO}}</text>
              <view data-g="{{tcinfo}}" bindtap="tapAddCart" data-index="{{index}}">
                <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
              </view>
            </view>
            <text class="sprice">￥{{sns.price}}</text>
            <text class="spec">{{sns.title}} </text>
          </view>
          <view class="foodspecs" wx:if="{{!tcinfo.hasSn}}">
             <view class="tnum">
              <view class="sdec {{newmap[tcinfo.id].NO>0?'show':''}}" data-tcinfo="{{tcinfo}}"  bindtap="tapDecCart">
                <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
              </view>
              <text class="snum {{newmap[tcinfo.id].NO>0?'show':''}}">{{newmap[tcinfo.id].NO}}</text>
              <view data-g="{{tcinfo}}" wx:if="{{tcinfo.total>0}}" bindtap="tapAddCart" data-index="{{index}}">
                <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
              </view>
              <view data-g="{{tcinfo}}" wx:if="{{tcinfo.total<=0}}">
                <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
              </view>
            </view>
            <text class="sprice">￥{{tcinfo.nowPrice}}</text>
            <text class="spec">{{tcinfo.title}} </text>
          </view>
          <view class="tyouhui" wx:if="{{tcinfo.youhui}}"><text>惠</text>{{tcinfo.youhui}}</view>
        </view>
      </view>
      <view class="shut_down" bindtap="tcdown">
        关闭
      </view>
    </view>

  <!--竖向-->
  <view wx:if="{{menutab==1}}" class="content-container">
      <scroll-view class="classify-container {{color}}classify-container" scroll-y="true">
        <view class="classify" wx:for="{{tabs}}" wx:for-item="classify" wx:key="id" data-id="s{{classify.id}}" bindtap="tapClassify">
          <view class="name">{{classify.title}}</view>
        </view>
      </scroll-view>
      <scroll-view class="goods-container" scroll-y="true" scroll-into-view="{{classifyViewed}}">
      <!--推荐菜品-->
      <scroll-view scroll-x="true" class="tinfo2" wx:if="{{couponList.length>0}}">
        <view class="list {{color}}list" wx:for="{{couponList}}" wx:key="unique" bindtap="get">
          <view class="container">
            <text class="tit">{{item.type==1?'满减券':'现金券'}}</text>
            <view class="act" wx:if="{{item.full}}">满{{item.full}}<text style="font-size:24rpx;text-align:center">减{{item.value}}</text></view>
            <view class="act" wx:else>{{item.value}}元</view>
            <view class="bottom" data-id="{{item.actId}}" >立即领取</view>
          </view>
        </view>
    </scroll-view>
      <view wx:if="{{menu.top>0}}" id="stj">
          <view class="title">热门推荐</view>
          <view class="goods" wx:for="{{tjList}}" wx:for-item="id" wx:key="unique">
            <view class="pic" bindtap="tcshow" data-g="{{id}}">
              <text wx:if="{{id.isNew}}" class="new"></text>
              <image src="{{api+id.logo}}"></image>
            </view>
            <view class="name ellips is">{{id.title}}
              <text wx:if="{{id.hasXc}}" class="i-xc"></text>
              <text wx:for="{{id.spiceLevel}}" class="i-la" wx:key="unique"></text>
              <text wx:if="{{id.hasSuan}}" class="i-suan"></text>
            </view>
            <text class="price">￥{{id.nowPrice}}         <text wx:if="{{id.sns}}"> 起</text>
            </text>
            <text class="sold" wx:if="{{id.oldPrice}}">￥{{id.oldPrice}}</text>
            <view class="join">
              <text class="youhui" wx:if="{{newmap[id.id].NO>0&&id.youhui}}">惠</text>
              <text wx:else>{{id.youhui}}</text>
              <view class="cnum" wx:if="{{!id.sns}}">
                <view wx:if="{{id.total>0}}" class="addCart" data-g="{{id}}" bindtap="tapAddCart" data-index="{{index}}">
                  <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                </view>
                <view wx:if="{{id.total<=0}}" class="addCart">
                  <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
                </view>
                <view class="cnums {{newmap[id.id].NO>0?'show':''}}">
                  <text class="num">{{newmap[id.id].NO}}</text>
                  <view class="addCart" data-g="{{id}}" data-index="{{index}}" bindtap="tapDecCart">
                    <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                  </view>
                </view>
              </view>
            </view>
            <view class="shch">
              <view>
                <text class="time" wx:if="{{id.cookType!=''}}">[{{id.cookType}}]</text> <text wx:if="{{id.cookMinute>0}}">{{id.cookMinute}}分钟</text></view>
              <view class="number" wx:if="{{id.orderCnt>0}}">{{id.orderCnt}}点过</view>
            </view>
            <view class="foodspec" wx:for="{{id.sns}}" wx:for-item="sns" wx:key="unique">
              <text class="spec">{{sns.title}} </text>
              <text class="sprice">￥{{sns.price}}</text>
              <view class="cnum">
                <view class="addCart" data-g="{{id}}" bindtap="tapAddCart" data-index="{{index}}">
                  <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                </view>
                <view class="cnums {{newmap[id.id+'_'+id.sns[index].id].NO>0?'show':''}}">
                  <text class="num">{{newmap[id.id+'_'+id.sns[index].id].NO}}</text>
                  <view class="addCart" data-g="{{id}}" data-index="{{index}}" bindtap="tapDecCart">
                    <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                  </view>
                </view>
              </view>
            </view>
          </view>
        </view>
      

        <view wx:for="{{menu.cates}}" wx:for-item="classify" wx:key="unique" id="s{{classify.id}}">
          <view class="title">{{classify.title}}</view>
          <view class="goods" wx:for="{{classify.glist}}" wx:for-item="id" wx:key="unique">
            <view class="pic" bindtap="tcshow" data-g="{{id}}">
              <text wx:if="{{id.isNew}}" class="new"></text>
              <image src="{{api+id.logo}}"></image>
            </view>
            <view class="name ellips is">{{id.title}}
              <text wx:if="{{id.hasXc}}" class="i-xc"></text>
              <text wx:for="{{id.spiceLevel}}" class="i-la" wx:key="unique"></text>
              <text wx:if="{{id.hasSuan}}" class="i-suan"></text>
            </view>
            <text class="price">￥{{id.nowPrice}}         <text wx:if="{{id.sns}}"> 起</text>
            </text>
            <text class="sold" wx:if="{{id.oldPrice}}">￥{{id.oldPrice}}</text>
            <view class="join">
              <text class="youhui" wx:if="{{newmap[id.id].NO>0&&id.youhui}}">惠</text>
              <text wx:else>{{id.youhui}}</text>
              <view class="cnum" wx:if="{{!id.sns}}">
                <view wx:if="{{id.total>0}}" class="addCart" data-g="{{id}}" bindtap="tapAddCart" data-index="{{index}}">
                  <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                </view>
                <view wx:if="{{id.total<=0}}" class="addCart">
                  <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
                </view>
                <view class="cnums {{newmap[id.id].NO>0?'show':''}}">
                  <text class="num">{{newmap[id.id].NO}}</text>
                  <view class="addCart" data-g="{{id}}" data-index="{{index}}" bindtap="tapDecCart">
                    <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                  </view>
                </view>
              </view>
            </view>
            <view class="shch">
              <view>
                <text class="time" wx:if="{{id.cookType!=''}}">[{{id.cookType}}]</text> <text wx:if="{{id.cookMinute>0}}">{{id.cookMinute}}分钟</text></view>
              <view class="number" wx:if="{{id.orderCnt>0}}">{{id.orderCnt}}点过</view>
            </view>
            <view class="foodspec" wx:for="{{id.sns}}" wx:for-item="sns" wx:key="unique">
              <text class="spec">{{sns.title}} </text>
              <text class="sprice">￥{{sns.price}}</text>
              <view class="cnum">
                <view class="addCart" data-g="{{id}}" bindtap="tapAddCart" data-index="{{index}}">
                  <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                </view>
                <view class="cnums {{newmap[id.id+'_'+id.sns[index].id].NO>0?'show':''}}">
                  <text class="num">{{newmap[id.id+'_'+id.sns[index].id].NO}}</text>
                  <view class="addCart" data-g="{{id}}" data-index="{{index}}" bindtap="tapDecCart">
                    <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                  </view>
                </view>
              </view>
            </view>
          </view>
        </view>
      </scroll-view>
    </view>
<!--底部价格-->
   <view class="cart">
      <view class="data" bindtap="{{newmap.amount>0?'showCartDetail':''}}">
        <view class="icon {{color}}ba_color" wx:if="{{!showCartDetail}}">
          <image src="{{api}}res/xcx/images/icon_cart.png"></image>
          <view class="count" wx:if="{{newmap.total>0}}">{{newmap.total}}</view>
        </view>
      </view>
      <view class="info {{showCartDetail?'changeinfo':''}}" bindtap="{{menu.amount>0?'showCartDetail':''}}">
        <view class="left {{showCartDetail?'left_width':''}}">
        <view class="nums">
          <text wx:if="{{newmap.total>0}}">已点{{newmap.total}}个</text>
          <text wx:else>请选择商品</text>
          <text class="{{showCartDetail?'over_width':'overflow'}}">
          <text wx:for="{{menuInfo.gCates}}" wx:key="unique" wx:if="{{item.num>0}}">{{item.title}}<text class="{{color}}text_color">{{item.num}}</text></text>
          </text>
        </view>
        <view class="total" wx:if="{{newmap.amount>0}}">￥{{newmap.amount}}</view>
        </view>
        <form report-submit="true" bindsubmit="confirm" wx:if="{{isWm==0}}">
          <button class="submitbutton {{color}}ba_color" formType="submit">确定</button>
        </form>
        <form report-submit="true" bindsubmit="{{newmap.amount>=menuInfo.wmLimit?'confirm':''}}" wx:if="{{isWm}}">
          <button class="{{newmap.amount>=menuInfo.wmLimit?color+'ba_color submitbutton':'nobutton'}}" formType="submit"><text wx:if="{{newmap.amount>=menuInfo.wmLimit}}">确定</text><text wx:else>￥{{menuInfo.wmLimit}}起送</text></button>
        </form>
      </view>
    </view>
<!--菜单详情-->
<view class="cart-detail {{showCartDetail ?'cart-show':''}}">
      <view class="mask" bindtap="hideCartDetail"></view>
      <view class="carticon">
        <image class="bg_gwc" src="{{api}}res/xcx/images/{{color}}bg_gwc.png"></image>
        <image class="gwc" src="{{api}}res/xcx/images/icon_cart.png"></image>
        <text>{{newmap.total}}</text>
      </view>
      <view class="tabinfo" wx:if="{{deskCode}}">
        <image src="{{api}}res/xcx/images/{{color}}bg_tabnum.png"></image>
        <view class="nr">NO.
          <text>{{deskCode}}</text></view>
      </view>
      <scroll-view class="list" scroll-y>
        <view class="empty">
          <text>购物车</text>
          <text catchtap="empty">一键清空</text>
        </view>
         <view wx:for="{{menu.cates}}" wx:key="unique" wx:for-item="cates" wx:for-index="cindex">
          <view class="item" wx:for="{{cates.glist}}" wx:key="id" wx:if="{{newmap[item.id].NO>0}}">
            <view class="img">
              <image src="{{api+item.logo}}"></image>
            </view>
            <view class="nr">
              <view class="name ellipsis">{{item.title}}</view>
              <view class="total">￥{{newmap[item.id].PR}}
                <text class="sprice" wx:if="{{item.oldPrice}}">￥{{item.oldPrice}}</text>
              </view>
              <view class="info" wx:for="{{item.sns}}" wx:for-item="sns" wx:key="unique" wx:if="{{newmap[item.id+'_'+sns.id].NO>0}}">
                <text class="speci">{{sns.title}}</text>
                <view class="oper">
                  <text class="add {{color}}cba_color" data-g="{{item}}" data-index="{{index}}" bindtap="tapAddCart">+</text>
                  <text class="tnum">{{newmap[item.id+'_'+sns.id].NO}}</text>
                  <text class="reduce {{color}}cba_color" data-g="{{item}}" data-index="{{index}}" bindtap="tapDecCart">-</text>
                </view>
              </view>
              <view class="info"  wx:if="{{!item.sns}}">
                <text></text>
                <view class="oper">
                  <text class="add {{color}}cba_color" data-g="{{item}}" data-index="{{index}}" bindtap="tapAddCart">+</text>
                  <view class="tnum">{{newmap[item.id].NO}}</view>
                  <text class="reduce {{color}}cba_color" data-g="{{item}}" data-index="{{index}}" bindtap="tapDecCart">-</text>
                </view>
              </view>
            </view>
          </view>
        </view> 
      </scroll-view>   
    </view>
  <!--确认点餐-->
  <view class="confirm-container {{confirmslid ? 'slidrihgt' : ''}}">
    <scroll-view scroll-y="true" class="scroll-hei">
      <view class="head {{color}}ba_color">
         <view class="title">{{menuInfo.storeName}}{{deskCode}}</view> 
        <navigator url="../index/dc?change=1">
          <view class="Modify">
            <image src="../../images/icon_edit.png"></image>修改桌号</view>
        </navigator>
        <view class="miss {{color}}ctext_color">请仔细检查桌号信息，如有错误请扫描二维码更改</view>
