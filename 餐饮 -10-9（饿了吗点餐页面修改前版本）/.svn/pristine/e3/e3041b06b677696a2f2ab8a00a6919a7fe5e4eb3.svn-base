        <view class="ititle" wx:if="{{order.historys}}">
          本次点菜（￥{{order.nowAmount}}元）
        </view>
        <view class="packpage" wx:for="{{order.list}}" wx:key="unique">
          <view class="foodimg">
            <image src="{{api+item.img}}"></image>
          </view>
          <view class="foodinfo">
            <view class="left">
              <text>{{item.title}}</text>
              <text class="nr">{{item.sn}}</text>
            </view>
            <view class="right">
              <text>{{item.num}}</text>
              <text>￥{{item.price}}</text>
            </view>
          </view>
        </view>
      </view>

      <!--历史菜单-->
      <view class="lcontainer">
        <view class="curmenu">
          <view class="{{sliding ? 'ltitle':'ltitleup'}}" bindtap="silde" wx:if="{{order.historys.length>0}}">历史菜单（￥{{order.historyAmount}}）</view>
          <view class="{{sliding ? 'slidhide' : 'slidnone'}}">
            <view class="packpage" wx:for="{{order.historys}}" wx:key="unique">
              <view class="foodimg">
                <image src="{{api+item.img}}"></image>
              </view>
              <view class="foodinfo">
                <view class="left">
                  <text>{{item.title}}</text>
                  <text class="nr">{{item.sn}}</text>
                </view>
                <view class="right">
                  <text>{{item.num}}</text>
                  <text>￥{{item.price}}</text>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
      <view class="allprice">
        <text class="gray">总计：</text>{{order.amount}}元
      </view>
      <view class="confirminfo">
