.cheader{
  height: 130rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  background-size: 20rpx 31rpx;
  font-size: 32rpx;
  line-height:50rpx;
}
.cheader view{
  max-width: 660rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.current{
  padding-left: 35rpx;
  background: url(https://img.zhcanting.com/res/xcx/images/wm_locab.png) left center no-repeat;
  background-size: 21rpx 28rpx;
}
.time{
  display: flex;
  justify-content:space-between;
  align-items: center;
  height: 90rpx;
  padding: 0 24rpx;
  font-size: 32rpx;
  background: #
}
.time .arrow{
  background: url(https://img.zhcanting.com/res/xcx/images/go_store.png) right center no-repeat;
  background-size: 18rpx 28rpx;
  font-size: 30rpx;
  color: #aaaaaa;
  padding-right: 40rpx;
}
.titl{
  height: 85rpx;
  line-height: 85rpx;
  color: #fff;
  text-align: center;
  font-size: 32rpx;
  background: #f89d45 url(https://img.zhcanting.com/res/xcx/images/snum_menu.png) 24rpx center no-repeat;
  background-size: 36rpx 34rpx;
}
.menu{
  display: flex;
  justify-content: row;
  margin-left: 24rpx;
  padding: 22rpx 24rpx 22rpx 0;
  border-bottom: 1px solid #ececec;
}
.menu:last-child{
  border: none;
}
.content{
  background: #f7f7f7;
}
.menu-container{
   height: 0px;
   overflow: hidden;
   transition: overflow 0.3ms linear both;
}
.slidup{
  height: auto;
}
.menu image{
  width: 130rpx;
  height: 130rpx;
}
.menu .info{
  width: 560rpx;
  margin-left: 10rpx;
}
.menu .tit{
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.menu .num{
  color: #aaa;
  font-size: 28rpx;
}
.menu .detail{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #888;
}
.price{
  font-size: 32rpx;
  color: #ff4f39;
}
.nr{
  display: flex;
  margin-left: 24rpx;
  border-bottom: 1px solid #ececec;
  height: 85rpx;
  align-items: center;
  justify-content:space-between;
  font-size: 32rpx;
  padding-right: 24rpx;
}
.line{
  display: flex;
  justify-content: space-between;
  color: #fff;
  font-size: 32rpx;
}
.line:first-child{
  margin-bottom: 10rpx;
}
.nr .gray,.lastnr .gray{
  color: #aaa;
  font-family: "Microsoft Yahei";
  text-align: right;
}
.nr input,.lastnr input{
  color: #aaa;
  width: 450rpx;
  text-align: right;
}
.cbtn{
    background-size: 29rpx 20rpx;
    color: #fff;
    border-radius: 47rpx;
    padding: 0rpx 23.5rpx 0rpx 60rpx;
    display: inline-block;
    height: 47rpx;
    line-height: 47rpx;
}

.bh{
  padding-left:10rpx;
  color: #aaa;
  font-size: 28rpx;
}
.title{
    background: #fff;
    font-size: 30rpx;
    color: #888;
    text-align: center;
    padding: 20rpx 0px;
    display: flex;
    justify-content: space-between;
}
.title text{
    margin-left: 23.5rpx;
    background: url('https://img.zhcanting.com/res/xcx/images/snum_menu.png') left center no-repeat;
    background-size: 35rpx 34rpx;
    padding-left: 56rpx;
}
.packpage{
    margin: 23.5rpx;
    margin-bottom: 0px;
    display: flex;
    flex-direction: row;
    padding-bottom: 23.5rpx;
    border-bottom: 1px solid #e7e7e7;
}
.packpage:last-child{
    border: none;
}
.packpage .foodimg{
    flex: 2;
    margin-right: 21rpx;
    height: 129rpx;
    overflow: hidden;
}
.foodimg image{
    width: 100%;
    height: 100%;
}
.packpage .foodinfo{
    flex: 9;
    display: flex;
    justify-content: space-between;
    padding-top: 15rpx;
}
.packpage .foodinfo{
    flex: 9;
    display: flex;
    justify-content: space-between;
    padding-top: 15rpx;
}
.foodinfo text{
    display: block;
    font-size: 30.5rpx;
    line-height: 45rpx;
}
.foodinfo .nr{
    color: #888;
    font-size: 28rpx;
}
.foodinfo .right{
    text-align: right;
}
.foodinfo .right text:first-child{
    font-size: 28rpx;
    color: #aaa;
}
.foodinfo .right text:last-child{
    color: #ff4f39;
}
.foodinfo .taste{
    margin-left: 1px;
    display: inline-block;
    padding: 0 23.5rpx;
    color: #fff;
    font-size: 23.5rpx;
    line-height: 37.5rpx;
    text-align: center;
    border-radius:35rpx;
    overflow: hidden;
}
.allprice {
    font-size: 30rpx;
    background: #f8f8f8;
    text-align: right;
    line-height:47rpx;
    padding: 20rpx 23.5rpx;
}
.cancel{
    font-size: 28rpx;
    display: inline-block;
    padding-left: 45rpx;
    zoom: 1; 
     background: url('https://img.zhcanting.com/res/xcx/images/order_finsh.png') left center no-repeat;
     background-size:30rpx 20rpx;
}
.hasPay{
    font-size: 28rpx;
    color: #35b220;
    display: inline-block;
    padding-left: 35rpx;
    zoom: 1; 
    background: url('https://img.zhcanting.com/res/xcx/images/wm_yzf.png') left center no-repeat;
    background-size:25rpx 18rpx;
}
.hasPick{
  font-size: 28rpx;
  display: inline-block;
  padding-left: 48rpx;
  zoom: 1; 
  background: url('https://img.zhcanting.com/res/xcx/images/wm_order.png') left center no-repeat;
  background-size:38rpx 36rpx;
}
.hascom{
  font-size: 28rpx;
  display: inline-block;
  padding-left: 48rpx;
  zoom: 1; 
  background: url('https://img.zhcanting.com/res/xcx/images/wm_wc.png') left center no-repeat;
  background-size:38rpx 38rpx;
}
.count{
  font-size: 28rpx;
  color: #ff4f39;
  display: block;
  padding-right: 30rpx;
}
.dotime{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.caneldd{
  margin: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 65rpx;
  border-radius: 32.5rpx;
  font-size: 28rpx;
}