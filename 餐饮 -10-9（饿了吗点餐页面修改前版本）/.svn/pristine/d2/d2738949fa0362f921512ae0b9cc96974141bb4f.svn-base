    height: 115rpx;
    border-top: 1px solid #DCDCDC;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content:space-between;
            justify-content:space-between;
    font-size: 32.8rpx;
    line-height: 63rpx;
}
.footer view:first-child{
     background:url('https://img.zhcanting.com/res/xcx/images/order_back.png') left center no-repeat;
     background-size: 15rpx 29rpx;
