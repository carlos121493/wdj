/* pages/index/ydwz.wxss */
page{
    background: #f8f8f8;
}
.res{
  margin:12rpx;
  margin-bottom:0;
  background:#E4EAFF;
  padding:21rpx;
  border-radius:18.75rpx 18.75rpx 0 0;
  -webkit-border-radius:18.75rpx 18.75rpx 0 0;
  border:1px dashed #BFCDFF;
  border-bottom:0 none;
  display:-webkit-flex;
  display:flex;
  -webkit-flex-direction:row;
          flex-direction:row;
}
.resimg{
  -webkit-flex: 3;
          flex: 3;
  margin-right: 21rpx;
  height: 164rpx;
}
.resimg image{
    width: 100%;
    height: 100%;
}
.resinfo{
  width: 500rpx;
}
.resinfo text{
  margin-top: 15rpx;
  font-size: 28rpx;
  display:block;
  white-space:nowrap; 
  overflow:hidden;
  text-overflow:ellipsis;
}
.resinfo text:last-child{
  font-size: 25.5rpx;
}
.restitle{
  font-size: 30.5rpx;
}
.numinfo{
  padding: 15rpx;
  background: #fff;
  border-bottom: 1px solid #dcdcdc;
}
.list{
    background: #fff;
}
.list .name{
  display: -webkit-flex;
  display: flex;
  width: 80rpx;
}
.list view{
    margin: 0 23.5rpx;
    border-bottom: 1px solid #eaeaea;
    line-height: 88rpx;
    font-size: 33rpx;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
            justify-content: space-between;
}
.gray{
    color: #aaa;
    background:url('https://img.zhcanting.com/res/xcx/images/order_right.png') right center no-repeat;
    background-size: 17.5rpx 30.5rpx;
    padding-right: 40rpx;
}
.list view input{
    text-align: right;
    margin-top: 17rpx;
    color: #aaaaaa;
    font-family: Microsoft Yahei;
}
.container{
    position: fixed;
    background-color: rgba(0, 0, 0, 0);  
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 99;
    display: none;
}
.containertrue{
    display: block;
    position: fixed;
    background-color: rgba(0, 0, 0, 0.4);  
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 99;
}
.timetrue{
    -webkit-animation: timetrue .2s linear;
            animation: timetrue .2s linear;
}
@-webkit-keyframes timetrue{
    from{
        -webkit-transform:  translateY(100%);
                transform:  translateY(100%);
    }
    to{
        -webkit-transform:  translateY(0%);
                transform:  translateY(0%);
    }
}
@keyframes timetrue{
    from{
        -webkit-transform:  translateY(100%);
                transform:  translateY(100%);
    }
    to{
        -webkit-transform:  translateY(0%);
                transform:  translateY(0%);
    }
}
.timefalse{
  -webkit-animation: timefalse .2s linear;
          animation: timefalse .2s linear;
}
@-webkit-keyframes timefalse{
    from{
        -webkit-transform:  translateY(0%);
                transform:  translateY(0%);
    }
    to{
        -webkit-transform:  translateY(100%);
                transform:  translateY(100%);
    }
}
@keyframes timefalse{
    from{
        -webkit-transform:  translateY(0%);
                transform:  translateY(0%);
    }
    to{
        -webkit-transform:  translateY(100%);
                transform:  translateY(100%);
    }
}
.time ,.seat{
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: auto;
    background: #fff;
    z-index: 999;
    opacity: 1;
    color: #262626;
    width: 100%;
}
.time .title,.seat .title{
    color: #aaa;
    font-size: 33rpx;   
    padding: 23.5rpx;
}
.time .close,.seat .close{
    background:url('https://img.zhcanting.com/res/xcx/images/icon_close.png') right center no-repeat;
    background-size: 30.5rpx;
    width: 30.5rpx;
    height: 30.5rpx;
    float: right;
    margin-top: 5rpx;
}
.time .title text{
    background:url('https://img.zhcanting.com/res/xcx/images/icon_time.png') left center no-repeat;
    background-size: 45rpx;
    padding-left: 55rpx;
}
.seat .title text{
  background:url('https://img.zhcanting.com/res/xcx/images/icon_place.png') left center no-repeat;
    background-size: 41rpx;
    padding-left: 55rpx;
}
.slidhide{
    -webkit-animation: slidown .5s ease-in both;
            animation: slidown .5s ease-in both;
}
.time .cnr{
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
            flex-direction: row;
}
.time .year{
    background: #fafafa;
    -webkit-flex: 8;
            flex: 8;
    border-right: 1px solid #ddd;
    height: 610rpx;
}
.time .year text{
    display: block;
    line-height: 75.5rpx;
    text-align: center;
    height: 75.5rpx;
    font-size: 32.8rpx;
    border-top: 1px solid #ececec;
}
.time .day{
    -webkit-flex: 4;
            flex: 4;
    background: #fafafa;
    height: 610rpx;
}
.time .day text{
    display: block;
    line-height: 75.5rpx;
    text-align: center;
    height: 75.5rpx;
    font-size: 32.8rpx;
    border-bottom: 1px solid #ececec;
}
.time .day .daytitle{
    height: 65rpx;
    line-height: 65rpx;
    color: #fff;
}
.time .selectrue{
    background:#faf0db url('https://img.zhcanting.com/res/xcx/images/icon_choose.png') 40rpx center no-repeat;
    background-size:66.7rpx 46.8rpx; 
}
.selectfalse{
    background: #fafafa;
}
.tabTit{background: #fafafa;}
.tabTit .active{
    background:#faf0db url('https://img.zhcanting.com/res/xcx/images/icon_choose.png') 40rpx center no-repeat;
    background-size:66.7rpx 46.8rpx;
     }
@-webkit-keyframes slidown{
    from{
        -webkit-transform:  translateY(100%);
                transform:  translateY(100%);
    }
    to{
        -webkit-transform:  translateY(0%);
                transform:  translateY(0%);
    }
}
@keyframes slidown{
    from{
        -webkit-transform:  translateY(100%);
                transform:  translateY(100%);
    }
    to{
        -webkit-transform:  translateY(0%);
                transform:  translateY(0%);
    }
}
.slidnone{
  display: none;
  -webkit-animation: slidoup .5s ease-in both;
          animation: slidoup .5s ease-in both;
}
@-webkit-keyframes slidup{
    from{
        -webkit-transform:  translateY(0%);
                transform:  translateY(0%);
    }
    to{
        -webkit-transform:  translateY(100%);
                transform:  translateY(100%);
    }
}
@keyframes slidup{
    from{
        -webkit-transform:  translateY(0%);
                transform:  translateY(0%);
    }
    to{
        -webkit-transform:  translateY(100%);
                transform:  translateY(100%);
    }
}
/*选择座位*/
.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.4);
  display: none;
}
.zan-dialog__container {
  position: fixed;
  bottom: 0;
  width: 100%;
  background: white;
  -webkit-transform: translateY(150%);
          transform: translateY(150%);
  transition: all 0.4s ease;
  z-index: 999;
}
.zan-dialog--show .zan-dialog__container {
  -webkit-transform: translateY(0);
          transform: translateY(0);
}
.zan-dialog--show .zan-dialog__mask {
  display: block;
}
