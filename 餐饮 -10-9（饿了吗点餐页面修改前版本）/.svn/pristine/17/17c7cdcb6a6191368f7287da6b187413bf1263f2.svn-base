/*支付公共样式*/
.paysuccess
{
  position: relative;
}
.paysuccess>image{
  width: 100%;
  height: 36rpx;
  position: absolute;
  top: 0;
}
.paysuccess .main {
    padding-top:110rpx;
}
.paysuccess .main .smpic {
    width:180rpx;
    height:180rpx;
    display: block;
    margin: 0 auto;
    overflow: hidden;
}
.paysuccess .main .smpic image{
    height: 180rpx;
    width: 180rpx;
}
.paysuccess .main .pic {
    width:210rpx;
    height:180rpx;
    display: block;
    margin: 0 auto;
    overflow: hidden;
}
.paysuccess .main .pic image{
    width: 100%;
    height: 100%;
}
.paysuccess .pictext {
    margin-top: 25rpx;
}
.paysuccess .pictext text{
    display: block;
    text-align: center;
    font-size:18px;
    color: #868686;
}
.paysuccess .mess {
    width: 390rpx;
    height:72rpx;
    display: block;
    background: #FCFEFF;
    border: 1px solid #E2E9FF;
    margin: 0 auto;
    text-align: center;
    padding: 10rpx 0px;
}
.paysuccess .mess .bg-zf {
    display: inline-block;
    vertical-align: top;
    height: 72rpx;
    line-height: 72rpx;
    color: #639BF0;
    font-size: 21px;
    background: url('https://img.zhcanting.com/res/xcx/images/icon_suceepay.png') left center no-repeat;
    background-size:54rpx 52.5rpx;
    padding-left: 68rpx;
}
.transparent{
  opacity:0.8;
}
/*****外卖公共样式*******/
.wmfooter{
  position:fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 114rpx;
  border-top: 1px solid #dbdbdb;
  background: #fff;
  padding: 0 24rpx;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-align-items: center;
          align-items: center;
  z-index: 99;
}
.back{
  font-size: 16px;
  padding-left: 35rpx;
  background: url('https://img.zhcanting.com/res/xcx/images/order_back.png') left center no-repeat;
  background-size:18rpx 30rpx; 
}
.xddlogo{
  display:-webkit-flex;
  display:flex;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-align-items: center;
          align-items: center;
  height: 100rpx;
}
.xddlogo .yds{
  width: 278rpx;
  height: 25rpx;
}
.xddlogo .xdd{
  width: 278rpx;
  height: 37rpx;
}
.xddlogo text{
  font-size: 12px;
  color: #aaa;
  display: inline-block;
  padding-left: 10rpx;
}
.tabars{
  display: -webkit-flex;
  display: flex;
  -webkit-flex-direction: row;
          flex-direction: row;
  font-size: 15px;
  width: 100%;
  position:fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 115rpx;
  background: #fff;
  padding-top: 15rpx;
}
.tabars view{
  display: -webkit-flex;
  display: flex;
  -webkit-flex:4;
          flex:4;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-align-items: center;
          align-items: center;
  color: #999;
}
.tabars image{
  width: 48rpx;
  height: 52rpx;
}
/*tabbar*/
.tabbox{
  height: 120rpx;
  width: 100%;
}
.tabbar_box{
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: row;
            flex-direction: row;
    -webkit-justify-content: space-around;
            justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 115rpx;
}
.tabbar_nav{
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-align-items: center;
            align-items: center;
    font-size: 14px;
    height: 100%;
}

.tabbar_icon{
    width: 52rpx;
