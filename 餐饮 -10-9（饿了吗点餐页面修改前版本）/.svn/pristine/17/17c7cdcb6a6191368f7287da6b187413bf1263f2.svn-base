}
 .example {
  display: block;
  height: 82rpx;
  line-height: 82rpx;
  width: 700rpx;
  margin: 0 25rpx;
  background: url('https://img.zhcanting.com/res/xcx/images/snum_right.png') right center no-repeat;
  background-size: 15rpx 27rpx;
}
.marquee_box {
  width: 675rpx;
  height: 82rpx;
  line-height: 82rpx;
  position: relative;
  overflow: hidden;
}

.marquee_text {
  white-space: nowrap;
  position: absolute;
  top: 0;
}
.loadings{
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 99999;
  background: #fff;
}
.loadings image{
  width: 60px;
  height: 60px;
}
