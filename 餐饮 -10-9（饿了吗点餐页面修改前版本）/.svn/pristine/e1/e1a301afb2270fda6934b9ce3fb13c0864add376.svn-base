/* pages/waimai/qrdc.wxss */
.header{
  height: 130rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #fff;
  background-size: 20rpx 31rpx;
  font-size: 32rpx;
  line-height:50rpx;
}
.header view{
  max-width: 660rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.current{
  padding-left: 35rpx;
  background: url(https://img.zhcanting.com/res/xcx/images/wm_locab.png) left center no-repeat;
  background-size: 21rpx 28rpx;
}
.time{
  display: flex;
  justify-content:space-between;
  align-items: center;
  height: 90rpx;
  padding: 0 24rpx;
  font-size: 32rpx;
  border-bottom:1px solid #dcdcdc; 
}
.time .arrow{
  background: url(https://img.zhcanting.com/res/xcx/images/go_store.png) right center no-repeat;
  background-size: 18rpx 28rpx;
  font-size: 30rpx;
  color: #aaaaaa;
  padding-right: 40rpx;
}
.titl{
  height: 85rpx;
  line-height: 85rpx;
  text-align: left;
  font-size: 32rpx;
  background: url(https://img.zhcanting.com/res/xcx/images/snum_menu.png) 24rpx center no-repeat;
  background-size: 36rpx 34rpx;
  padding-left: 76rpx;
  overflow: hidden; 
  text-overflow: ellipsis;
  white-space: nowrap;
}
.menu{
  display: flex;
  justify-content: row;
  margin-left: 24rpx;
  padding: 22rpx 24rpx 22rpx 0;
  border-bottom: 1px solid #ececec;
}
.menu:last-child{
  border: none;
}
.menu image{
  width: 130rpx;
  height: 130rpx;
}
.menu .info{
  width: 560rpx;
  margin-left: 10rpx;
}
.menu .tit{
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.menu .num{
  color: #aaa;
  font-size: 28rpx;
}
.menu .detail{
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #888;
}
.price{
  font-size: 32rpx;
  color: #ff4f39;
}
.nr{
  display: flex;
  margin-left: 24rpx;
  border-bottom: 1px solid #ececec;
  height: 85rpx;
  align-items: center;
  justify-content:space-between;
  font-size: 32rpx;
  padding-right: 24rpx;
}
.mj{
  color: #8ea8ff;
}
.mj text{
  color: #aaa;
}
.dj{
  background:url(https://img.zhcanting.com/res/xcx/images/go_store.png) right center no-repeat;
  background-size:18rpx 28rpx;
  padding-right: 40rpx;
}
.dj text{
  background: #8ea8ff url(https://img.zhcanting.com/res/xcx/images/gou_n.png) 10rpx center no-repeat;
  padding: 5rpx 20rpx 5rpx 55rpx;
  border-radius: 30rpx;
  color: #fff;
  background-size: 35rpx 25rpx;
}
.total{
  background: #8ea8ff;
  padding: 24rpx;
}
.line{
  display: flex;
  justify-content: space-between;
  color: #fff;
  font-size: 32rpx;
}
.line:first-child{
  margin-bottom: 10rpx;
}
.nr .gray,.lastnr .gray{
  color: #aaa;
  font-family: "Microsoft Yahei";
  text-align: right;
}
.nr input,.lastnr input{
  color: #aaa;
  width: 450rpx;
  text-align: right;
}
.lastnr{
  display: flex;
  padding-left: 24rpx;
  border-bottom: 1px solid #ececec;
  height: 85rpx;
  align-items: center;
  justify-content:space-between;
  font-size: 32rpx;
  padding-right: 24rpx;
}
.fprice {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.fprice text{
  color: #ff4f39;
  font-size: 34rpx;
  padding-right: 15rpx;
}
/***送餐时间***/
.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.4);
  display: none;
}
.zan-dialog__container {
  position: fixed;
  bottom: 0;
  width: 100%;
  background: white;
  transform: translateY(150%);
  transition: all 0.4s ease;
  z-index: 999;
}
.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}
.zan-dialog--show .zan-dialog__mask {
  display: block;
}
.title{
  color:#aaa;
font-size:33rpx;
padding:23.5rpx;

}
.close{
    background:url('https://img.zhcanting.com/res/xcx/images/icon_close.png') right center no-repeat;
    background-size: 30.5rpx;
    width: 30.5rpx;
    height: 30.5rpx;
    float: right;
    margin-top: 5rpx;
}
.title text{
    background:url('https://img.zhcanting.com/res/xcx/images/icon_time.png') left center no-repeat;
    background-size: 40rpx;
    padding-left: 55rpx;
}
.tabTit{background: #fafafa;}
.tabTit .active{
    background:#faf0db url('https://img.zhcanting.com/res/xcx/images/icon_choose.png') right 20rpx center no-repeat;
    background-size:66.7rpx 46.8rpx;
     }
@keyframes slidown{
    from{
        transform:  translateY(100%);
    }
    to{
        transform:  translateY(0%);
    }
}
.times .nr1{
    width: 100%;
    display: flex;
    flex-direction: row;
    border-bottom: 11.7rpx solid #8ea8ff;
}
.times .year{
    background: #fafafa;
