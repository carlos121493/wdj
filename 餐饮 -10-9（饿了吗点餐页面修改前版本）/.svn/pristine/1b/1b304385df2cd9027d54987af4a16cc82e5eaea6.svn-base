/* pages/waimai/dzxz.wxss */
page{
  height: 100%;
  background: #f9f9f9;
}
.dheader{
  height: 90rpx;
  display: flex;
  justify-content: row;
  align-items: center;
  padding: 0 20rpx;
}
.address{
  padding-right: 45rpx;
  color: #fff;
  font-size: 28rpx;
  background-size: 19rpx 17rpx;
  margin-right: 60rpx;
}
.search{
  width: 450rpx;
  height: 55rpx;
  color: #fff;
  line-height: 55rpx;
  border-radius: 27.5rpx;
  background: rgba(0,0,0,.1) url('https://img.zhcanting.com/res/xcx/images/wm_ss.png') 20rpx center no-repeat;
  background-size: 28rpx 27rpx;
  padding:0 25rpx 0 65rpx;
  font-size: 26rpx;
}
.search input{
  height: 100%;
  line-height: 100%;
}
.content{
  padding-left: 24rpx;
  background: #fff;
}
.list{
  display: flex;
  flex-direction: row;
  align-items: center;
  height:  120rpx;
  font-size: 26rpx;
  padding-right: 24rpx;
  border-bottom: 1px solid #ececec;
}
.list:last-child{
  border: none;
}
.locat{
  width: 60rpx;
  background: url('https://img.zhcanting.com/res/xcx/images/wm_loca.png') left center no-repeat;
  background-size:28rpx 36rpx; 
  height: 100%;
}
.info{
  width: 640rpx;
}
.aess{
  color: #aaaaaa;
}
.chooseAdre{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  transform: translateX(100%);
  transition: all 0.3s;
  z-index: 999;
}
.cShow{
  transform: translateX(0%);
}
.container{
  width: 100%;
  height: 100%;
  font-size: 28rpx; 
}
.contact{
  display: flex;
  flex-direction: row;
  height: 88rpx;
  align-items: center;
  background: #fff;
  padding-left: 24rpx;
}
.text{
  width: 125rpx;
}
.gender{
  display: flex;
  flex-direction: row;
  height: 88rpx;
  align-items: center;
  padding-left: 125rpx;
  border-bottom: 1px solid #ececec;
  margin-left: 24rpx;
}
.gender text{
  font-size: 30.5rpx;
  padding: 7rpx 48rpx;
  background: #fff;
  border: 1px solid #bacfff;
  border-radius: 10rpx;
  margin-right: 10rpx;
}
.gender .sele{
  border: #5ac92a;
  background: #5ac92a url(https://img.zhcanting.com/res/xcx/images/wm_xz.png) 28rpx center no-repeat;
  background-size: 25rpx 22rpx;
  color: #fff;
  padding:7rpx 33rpx 7rpx 63rpx;
}
.mu-input{
  width: 575rpx;
  color: #888;
  font-size: 28rpx;
  margin-top: 5rpx;
}
.phone,.place,.house{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 88rpx;
  background: #fff;
  position: relative;
}
.place,.house{
  margin-top: 1px;
}
.phone::after,.place::after,.house::after{
  content: '';
  width: 726rpx;
  height: 1px;
  position: absolute;
  left: 24rpx;
  top: 88rpx;
  background: #ececec;
}
.phone text,.place text,.house text{
  width: 125rpx;
  padding-left: 24rpx;
}
.arrow{
  width: 535rpx;
  background: url(https://img.zhcanting.com/res/xcx/images/order_right.png) right center no-repeat;
  background-size: 20rpx 31rpx;
  padding-right: 40rpx;
  height:auto;
}
.default{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 88rpx;
  background: #fff;
  position: relative;
  margin-top: 1px;
}
.default::after{
  content: '';
  width: 750rpx;
  height: 1px;
  position: absolute;
  left: 0rpx;
  top: 88rpx;
  background: #ececec;
}
.default text{
  padding-left: 24rpx;
}
.tick{
  width: 47rpx;
  height: 47rpx;
  background: #ececec url(https://img.zhcanting.com/res/xcx/images/wm_xz.png) center center no-repeat;
  border-radius: 10rpx;
  margin-right: 24rpx;
  background-size: 25rpx 22rpx;
}
.setick{
  background: #5ac92a url(https://img.zhcanting.com/res/xcx/images/wm_xz.png) center center no-repeat;
  background-size: 25rpx 22rpx;
}
.btn{
  display: flex;
  flex-direction: row;
}
.del{
  font-size:28rpx;
  width:235rpx;
  line-height:63rpx;
  text-align:center;
  color:#fff;
  border-radius:31.5rpx;
  margin-right: 20rpx;
  background: #ff2929;
}