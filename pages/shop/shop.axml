<wxs module="m1">
  var wmType = function(item) {
    var wmType = 0;
    for (var i = 0; i < item.glist.length; i++) {
      if (item.glist[i].showType == 3||item.glist[i].showType == 1) {
        wmType = 3;
      }
    }
    return wmType;
  }

  var dnType = function(item) {
    var wmType = 0;
    for (var i = 0; i < item.glist.length; i++) {
      if (item.glist[i].showType == 2||item.glist[i].showType == 1) {
        wmType = 2;
      }
    }
    return wmType;
  } 
  
  module.exports.wmType = wmType;
  module.exports.dnType = dnType;

</wxs>
<wxs module="m2">
  var num=0;
  var tag=function(tabs,index,type){
  if(index>0){
      for(var i=0;i<=index;i++){
        var glist=tabs[i].data.glist;
        for (var j = 0; j < glist.length; j++) {
           if(glist[j].showType==type||glist[j].showType==1){
            num=0;
            break;
          }else{
            num=-1;
          } 
        }
        index+=num;
      }
    }
    return index;
  }
  module.exports.tag = tag;  
</wxs>