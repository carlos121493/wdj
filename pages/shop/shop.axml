<view class="container">
  <view class="loading" a:if="{{loading}}">
    <image src="../../images/hope.gif"></image>
  </view>
  <view class="dheader">
    <navigator url="../shop/hdsm">
      <image a:if="{{menuInfo.saleImg}}" src="{{api+menuInfo.saleImg}}"></image>
      <image a:else src="{{api}}res/xcx/images/deflt_banner.png"></image>
    </navigator>
    <view calss="header-info">{{menuInfo.saleInfo}}</view>
  </view>
  <!--横向-->
  <view class="page-body" a:if="{{menutab==2}}">
    <!--店内分类-->
     <view a:if="{{tab_config.fixed}}" class="tab-bar {{color}}tab-bar">
      <view a:for="{{tabs}}" a:key="unique" bindtap="handlerTabTap" data-index="{{m2.tag(tabs,index,2)}}" class="tab {{tab_config.active_tab==index?'{{color}}text_color':''}} " style="width: {{tab_config.item_width}}px;" a:if="{{isWm==0&&m1.dnType(item.data)==2}}">
        <text>{{item.title}}</text>
      </view>
      <view style="width: {{tab_config.item_width}}px; left: {{tab_config.underline.offset}}px;" class="under-line withAnimate"></view>
    </view>
    <scroll-view a:elif="{{isWm==0}}" class="tab-bar {{color}}tab-bar" scroll-x="true" bindscroll="onScroll" style="width: 100%;" scroll-left="{{tab_config.tab_left}}" >
      <view a:for="{{tabs}}" a:key="unique" data-index="{{m2.tag(tabs,index,2)}}" bindtap="handlerTabTap" class="tab {{tab_config.active_tab==index?'{{color}}ctext_color':''}} " style="width: {{tab_config.item_width}}px;" a:if="{{isWm==0&&m1.dnType(item.data)==2}}">
        <text>{{item.title}}</text>
      </view>
      <view style="width: {{tab_config.item_width}}px; left: {{tab_config.underline.offset}}px;" class="under-line withAnimate"></view>
    </scroll-view> 
    <!--外卖分类-->
    <view a:if="{{tab_config.fixed}}" class="tab-bar {{color}}tab-bar">
      <view a:for="{{tabs}}" a:key="unique" bindtap="handlerTabTap"  class="tab {{tab_config.active_tab==index?'{{color}}text_color':''}} " style="width: {{tab_config.item_width}}px;" a:if="{{isWm==1&&m1.wmType(item.data)==3}}">
        <text>{{item.title}}</text>
      </view>
      <view style="width: {{tab_config.item_width}}px; left: {{tab_config.underline.offset}}px;" class="under-line withAnimate"></view>
    </view>

    <scroll-view a:elif="{{isWm==1}}" class="tab-bar {{color}}tab-bar" scroll-x="true" bindscroll="onScroll" style="width: 100%;" scroll-left="{{tab_config.tab_left}}">
      <view a:for="{{tabs}}" a:key="unique" data-index="{{m2.tag(tabs,index,3)}}" bindtap="handlerTabTap" class="tab {{tab_config.active_tab==index?'{{color}}ctext_color':''}} " style="width: {{tab_config.item_width}}px;" a:if="{{isWm==1&&m1.wmType(item.data)==3}}">
        <text>{{item.title}}</text>
      </view>
      <view style="width: {{tab_config.item_width}}px; left: {{tab_config.underline.offset}}px;" class="under-line withAnimate"></view>
    </scroll-view>


    <view style="height:790rpx;background:#f2f2f2">
      <swiper class="swiper" indicator-dots="{{swipe_config.indicator_dots}}" autoplay="{{swipe_config.autoplay}}" interval="{{swipe_config.interval}}" duration="{{swipe_config.duration}}" current="{{swipe_config.current}}" bindchange="swiperChange">
      <!--店内推荐-->
        <block a:key="unique" a:if="{{menu.top>0&&isWm==0&&m1.dnType(tabs[0].data)==2}}">
          <swiper-item>
            <scroll-view scroll-y="true" style="height:790rpx;">
              <!--优惠券-->
              <scroll-view scroll-x="true" class="tinfo1" a:if="{{couponList.length>0}}">
                <view class="list {{color}}list" a:for="{{couponList}}" bindtap="get" a:key="unique">
                  <view class="container">
                    <text class="tit">{{item.type==1?'满减券':'现金券'}}</text>
                    <view class="act" a:if="{{item.full}}">满{{item.full}}
                      <text style="font-size:24rpx;text-align:center">减{{item.value}}</text>
                    </view>
                    <view class="act" a:else>{{item.value}}元</view>
                    <view class="bottom">立即领取</view>
                  </view>
                </view>
              </scroll-view>
              <view class="swiper-item">
              <!--店内推荐菜品-->
                <view class="list" a:for="{{tjList}}" a:if="{{isWm==0&&(item.showType==2||item.showType==1)}}" a:key="unique">
                  <view class="img" bindtap="tcshow" data-g="{{item}}">
                    <text a:if="{{item.isNew}}" class="new"></text>
                    <image src="{{api+item.logo}}"></image>
                  </view>
                  <view class="info">
                    <view class="name">
                      <text>{{item.title}}</text>
                      <text a:if="{{item.sns}}" class="yj {{color}}ctext_color" data-g="{{item}}" bindtap="tcshow">多规格</text>
                    </view>
                    <view class="num">
                      <text class="time" a:if="{{item.cookType!=''}}">[{{item.cookType}}] <text a:if="{{item.cookMinute>0}}">{{item.cookMinute}}分钟</text></text>
                      <text class="number" a:if="{{item.orderCnt>0}}">{{item.orderCnt}}点过</text>
                    </view>
                    <view class="price">
                      <text>￥{{item.nowPrice}}</text>
                      <view class="calculate">
                        <view class="cnums {{newmap[item.id].NO>0?'show':''}}">
                          <view class="addCart" data-g="{{item}}" data-index="{{index}}" bindtap="{{item.sns?'tcshow':'tapDecCart'}}">
                            <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                          </view>
                        </view>
                        <text class="cnums nums {{newmap[item.id].NO>0?'show':''}}">{{newmap[item.id].NO}}</text>
                        <view class="addcart" a:if="{{item.total>0}}" data-g="{{item}}" bindtap="{{item.sns?'tcshow':'tapAddCart'}}" data-index="{{index}}">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                        </view>
                        <view a:if="{{item.total<=0}}" class="addcart">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
                        </view>
                      </view>
                    </view>
                  </view>
                </view>
            </view>
            </scroll-view>
          </swiper-item>
        </block>
      <!--外卖推荐-->
      <block a:key="unique" a:if="{{menu.top>0&&isWm==1&&m1.wmType(tabs[0].data)==3}}">
          <swiper-item>
            <scroll-view scroll-y="true" style="height:790rpx;">
              <!--优惠券-->
              <scroll-view scroll-x="true" class="tinfo1" a:if="{{couponList.length>0}}">
                <view class="list {{color}}list" a:for="{{couponList}}" bindtap="get" a:key="unique">
                  <view class="container">
                    <text class="tit">{{item.type==1?'满减券':'现金券'}}</text>
                    <view class="act" a:if="{{item.full}}">满{{item.full}}
                      <text style="font-size:24rpx;text-align:center">减{{item.value}}</text>
                    </view>
                    <view class="act" a:else>{{item.value}}元</view>
                    <view class="bottom">立即领取</view>
                  </view>
                </view>
              </scroll-view>
              <view class="swiper-item">
              <!--店内推荐菜品-->
                <view class="list" a:for="{{tjList}}" a:if="{{isWm==1&&(item.showType==3||item.showType==1)}}" a:key="unique">
                  <view class="img" bindtap="tcshow" data-g="{{item}}">
                    <text a:if="{{item.isNew}}" class="new"></text>
                    <image src="{{api+item.logo}}"></image>
                  </view>
                  <view class="info">
                    <view class="name">
                      <text>{{item.title}}</text>
                      <text a:if="{{item.sns}}" class="yj {{color}}ctext_color" data-g="{{item}}" bindtap="tcshow">多规格</text>
                    </view>
                    <view class="num">
                      <text class="time" a:if="{{item.cookType!=''}}">[{{item.cookType}}] <text a:if="{{item.cookMinute>0}}">{{item.cookMinute}}分钟</text></text>
                      <text class="number" a:if="{{item.orderCnt>0}}">{{item.orderCnt}}点过</text>
                    </view>
                    <view class="price">
                      <text>￥{{item.nowPrice}}</text>
                      <view class="calculate">
                        <view class="cnums {{newmap[item.id].NO>0?'show':''}}">
                          <view class="addCart" data-g="{{item}}" data-index="{{index}}" bindtap="{{item.sns?'tcshow':'tapDecCart'}}">
                            <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                          </view>
                        </view>
                        <text class="cnums nums {{newmap[item.id].NO>0?'show':''}}">{{newmap[item.id].NO}}</text>
                        <view class="addcart" a:if="{{item.total>0}}" data-g="{{item}}" bindtap="{{item.sns?'tcshow':'tapAddCart'}}" data-index="{{index}}">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                        </view>
                        <view a:if="{{item.total<=0}}" class="addcart">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
                        </view>
                      </view>
                    </view>
                  </view>
                </view>
            </view>
            </scroll-view>
          </swiper-item>
        </block>

        <block a:for="{{menu.cates}}" a:key="unique" a:for-item="cates">
        <!--店内商品-->
          <swiper-item a:if="{{isWm==0&&m1.dnType(cates)==2}}">
            <scroll-view scroll-x="true" class="tinfo1" a:if="{{couponList.length>0&&menu.top==0&&index==0}}">
              <view class="list {{color}}list" a:for="{{couponList}}" a:key="unique" bindtap="get">
                <view class="container">
                  <text class="tit">{{item.type==1?'满减券':'现金券'}}</text>
                  <view class="act" a:if="{{item.full}}">满{{item.full}}
                    <text style="font-size:24rpx;text-align:center">减{{item.value}}</text>
                  </view>
                  <view class="act" a:else>{{item.value}}元</view>
                  <view class="bottom">立即领取</view>
                </view>
              </view>
            </scroll-view>
            <scroll-view scroll-y="true" style="height:790rpx;">
              <view class="swiper-item">
                <view class="list" a:for="{{cates.glist}}" a:key="unique" a:if="{{isWm==0&&(item.showType==2||item.showType==1)}}">
                  <view class="img" bindtap="tcshow" data-g="{{item}}">
                    <text a:if="{{item.isNew}}" class="new"></text>
                    <image src="{{api+item.logo}}"></image>
                  </view>
                  <view class="info">
                    <view class="name">
                      <text>{{item.title}}</text>
                      <text a:if="{{item.sns}}" class="yj {{color}}ctext_color" data-g="{{item}}" bindtap="tcshow">多规格</text>
                    </view>
                    <view class="num">
                      <text class="time" a:if="{{item.cookType!=''}}">[{{item.cookType}}] <text a:if="{{item.cookMinute>0}}">{{item.cookMinute}}分钟</text></text>
                      <text class="number" a:if="{{item.orderCnt>0}}">{{item.orderCnt}}点过</text>
                    </view>
                    <view class="price">
                      <text>￥{{item.nowPrice}}</text>
                      <view class="calculate">
                        <view class="cnums {{newmap[item.id].NO>0?'show':''}}">
                          <view class="addCart" data-g="{{item}}" data-index="{{index}}" bindtap="{{item.sns?'tcshow':'tapDecCart'}}">
                            <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                          </view>
                        </view>
                        <text class="cnums nums {{newmap[item.id].NO>0?'show':''}}">{{newmap[item.id].NO}}</text>
                        <view class="addcart" a:if="{{item.total>0}}" data-g="{{item}}" bindtap="{{item.sns?'tcshow':'tapAddCart'}}" data-index="{{index}}">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                        </view>
                        <view a:if="{{item.total<=0}}" class="addcart">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
                        </view>
                      </view>
                    </view>
                  </view>
                </view>
              </view>
            </scroll-view>
          </swiper-item>
        <!--外卖商品-->
        <swiper-item a:if="{{isWm==1&&m1.wmType(cates)==3}}">
            <scroll-view scroll-x="true" class="tinfo1" a:if="{{couponList.length>0&&menu.top==0&&index==0}}">
              <view class="list {{color}}list" a:for="{{couponList}}" a:key="unique" bindtap="get">
                <view class="container">
                  <text class="tit">{{item.type==1?'满减券':'现金券'}}</text>
                  <view class="act" a:if="{{item.full}}">满{{item.full}}
                    <text style="font-size:24rpx;text-align:center">减{{item.value}}</text>
                  </view>
                  <view class="act" a:else>{{item.value}}元</view>
                  <view class="bottom">立即领取</view>
                </view>
              </view>
            </scroll-view>
            <scroll-view scroll-y="true" style="height:790rpx;">
              <view class="swiper-item">
                <view class="list" a:for="{{cates.glist}}" a:key="unique" a:if="{{isWm==1&&(item.showType==3||item.showType==1)}}">
                  <view class="img" bindtap="tcshow" data-g="{{item}}">
                    <text a:if="{{item.isNew}}" class="new"></text>
                    <image src="{{api+item.logo}}"></image>
                  </view>
                  <view class="info">
                    <view class="name">
                      <text>{{item.title}}</text>
                      <text a:if="{{item.sns}}" class="yj {{color}}ctext_color" data-g="{{item}}" bindtap="tcshow">多规格</text>
                    </view>
                    <view class="num">
                      <text class="time" a:if="{{item.cookType!=''}}">[{{item.cookType}}] <text a:if="{{item.cookMinute>0}}">{{item.cookMinute}}分钟</text></text>
                      <text class="number" a:if="{{item.orderCnt>0}}">{{item.orderCnt}}点过</text>
                    </view>
                    <view class="price">
                      <text>￥{{item.nowPrice}}</text>
                      <view class="calculate">
                        <view class="cnums {{newmap[item.id].NO>0?'show':''}}">
                          <view class="addCart" data-g="{{item}}" data-index="{{index}}" bindtap="{{item.sns?'tcshow':'tapDecCart'}}">
                            <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                          </view>
                        </view>
                        <text class="cnums nums {{newmap[item.id].NO>0?'show':''}}">{{newmap[item.id].NO}}</text>
                        <view class="addcart" a:if="{{item.total>0}}" data-g="{{item}}" bindtap="{{item.sns?'tcshow':'tapAddCart'}}" data-index="{{index}}">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
                        </view>
                        <view a:if="{{item.total<=0}}" class="addcart">
                          <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
                        </view>
                      </view>
                    </view>
                  </view>
                </view>
              </view>
            </scroll-view>
          </swiper-item>
        </block>
      </swiper>
    </view>
  </view>
  <!--弹出详情-->
  <view class="tcontent {{tcshow?'':'tcshow'}}">
    <view class="nr">
      <!-- <swiper indicator-dots="{{indicatorDots}}"
        autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <block a:for="{{tcimgs}}">
          <swiper-item>
            <image src="{{api+item}}"/>
          </swiper-item>
        </block>
      </swiper> -->
      <swiper a:if="{{tcimgs!=''}}" indicator-dots="{{indicatorDots}}" vertical="{{vertical}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" class="banner">
        <block a:for="{{tcimgs}}" a:key="unique">
          <swiper-item>
            <image class="tcimgs" src="{{api+item}}" />
          </swiper-item>
        </block>
      </swiper>
      <view class="logo" a:if="{{tcimgs==''}}">
        <image src="{{api+tcinfo.logo}}"></image>
      </view>
      <view class="content">
        <view class="name">{{tcinfo.title}}
          <text a:if="{{tcinfo.hasXc}}" class="i-xc"></text>
          <text a:for="{{tcinfo.spiceLevel}}" class="i-la" a:key="unique"></text>
          <text a:if="{{tcinfo.hasSuan}}" class="i-suan"></text>
        </view>
        <view class="price">￥{{tcinfo.nowPrice}}
          <text a:if="{{tcinfo.oldPrice}}"> ￥{{tcinfo.oldPrice}}</text>
        </view>
        <view class="remark" a:if="{{tcinfo.remark}}">{{tcinfo.remark}}</view>
        <view class="num">
          <text class="time" a:if="{{tcinfo.cookType!=''}}">[{{tcinfo.cookType}}]</text>
          <text a:if="{{item.cookMinute>0}}">{{item.cookMinute}}分钟</text>
          <text class="number" a:if="{{tcinfo.orderCnt>0}}">{{tcinfo.orderCnt}}点过</text>
        </view>
        <view class="foodspecs" a:for="{{tcinfo.sns}}" a:for-item="sns" a:key="unique">
          <view class="tnum">
            <view class="sdec {{newmap[tcinfo.id+'_'+sns.id].NO>0?'show':''}}" data-tcinfo="{{tcinfo}}" data-index="{{index}}" bindtap="tapDecCart">
              <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
            </view>
            <text class="snum {{newmap[tcinfo.id+'_'+sns.id].NO>0?'show':''}}">{{newmap[tcinfo.id+'_'+sns.id].NO}}</text>
            <view data-g="{{tcinfo}}" bindtap="tapAddCart" data-index="{{index}}">
              <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
            </view>
          </view>
          <text class="sprice">￥{{sns.price}}</text>
          <text class="spec">{{sns.title}} </text>
        </view>
        <view class="foodspecs" a:if="{{!tcinfo.hasSn}}">
          <view class="tnum">
            <view class="sdec {{newmap[tcinfo.id].NO>0?'show':''}}" data-tcinfo="{{tcinfo}}" bindtap="tapDecCart">
              <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
            </view>
            <text class="snum {{newmap[tcinfo.id].NO>0?'show':''}}">{{newmap[tcinfo.id].NO}}</text>
            <view data-g="{{tcinfo}}" a:if="{{tcinfo.total>0}}" bindtap="tapAddCart" data-index="{{index}}">
              <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
            </view>
            <view data-g="{{tcinfo}}" a:if="{{tcinfo.total<=0}}">
              <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
            </view>
          </view>
          <text class="sprice">￥{{tcinfo.nowPrice}}</text>
          <text class="spec">{{tcinfo.title}} </text>
        </view>
        <view class="tyouhui" a:if="{{tcinfo.youhui}}">
          <text>惠</text>{{tcinfo.youhui}}</view>
      </view>
    </view>
    <view class="shut_down" bindtap="tcdown">
      关闭
    </view>
  </view>

  <!--竖向-->
  <view a:if="{{menutab==1}}" class="content-container">
    <scroll-view class="classify-container {{color}}classify-container" scroll-y="true">
      <!--外卖分类-->
      <view class="classify" a:for="{{tabs}}" a:for-item="classify" a:key="id" data-id="s{{classify.id}}" a:if="{{isWm==1&&m1.wmType(classify.data)==3}}" bindtap="tapClassify">
        <view class="name">{{classify.title}}</view>
      </view>
      <!--店内分类-->
      <view class="classify" a:for="{{tabs}}" a:for-item="classify" a:key="id" data-id="s{{classify.id}}" a:if="{{isWm==0&&m1.dnType(classify.data)==2}}" bindtap="tapClassify">
        <view class="name">{{classify.title}}</view>
      </view>
    </scroll-view>
    <scroll-view class="goods-container" scroll-y="true" scroll-into-view="{{classifyViewed}}">
      <!--推荐菜品-->
      <scroll-view scroll-x="true" class="tinfo2" a:if="{{couponList.length>0}}">
        <view class="list {{color}}list" a:for="{{couponList}}" a:key="unique" bindtap="get">
          <view class="container">
            <text class="tit">{{item.type==1?'满减券':'现金券'}}</text>
            <view class="act" a:if="{{item.full}}">满{{item.full}}
              <text style="font-size:24rpx;text-align:center">减{{item.value}}</text>
            </view>
            <view class="act" a:else>{{item.value}}元</view>
            <view class="bottom" data-id="{{item.actId}}">立即领取</view>
          </view>
        </view>
      </scroll-view>
      <!--外卖热门推荐-->
      <view a:if="{{menu.top>0&&isWm==1&&m1.wmType(tabs[0].data)==3}}" id="stj">
        <view class="title">热门推荐</view>
        <view class="goods" a:for="{{tjList}}" a:for-item="id" a:key="unique" a:if="{{isWm==1&&(id.showType==3||id.showType==1)}}">
          <view class="pic" bindtap="tcshow" data-g="{{id}}">
            <text a:if="{{id.isNew}}" class="new"></text>
            <image src="{{api+id.logo}}"></image>
          </view>
          <view class="name ellips is">{{id.title}}
            <text a:if="{{id.hasXc}}" class="i-xc"></text>
            <text a:for="{{id.spiceLevel}}" class="i-la" a:key="unique"></text>
            <text a:if="{{id.hasSuan}}" class="i-suan"></text>
          </view>
          <text class="price">￥{{id.nowPrice}}         <text a:if="{{id.sns}}"> 起</text>
          </text>
          <text class="sold" a:if="{{id.oldPrice}}">￥{{id.oldPrice}}</text>
          <view class="join">
            <text class="youhui" a:if="{{newmap[id.id].NO>0&&id.youhui}}">惠</text>
            <text a:else>{{id.youhui}}</text>
            <view class="cnum" a:if="{{!id.sns}}">
              <view a:if="{{id.total>0}}" class="addCart" data-g="{{id}}" bindtap="tapAddCart" data-index="{{index}}">
                <image src="{{api}}res/xcx/images/{{color}}icon_in.png"></image>
              </view>
              <view a:if="{{id.total<=0}}" class="addCart">
                <image src="{{api}}res/xcx/images/{{color}}icon_in_n.png"></image>
              </view>
              <view class="cnums {{newmap[id.id].NO>0?'show':''}}">
                <text class="num">{{newmap[id.id].NO}}</text>
                <view class="addCart" data-g="{{id}}" data-index="{{index}}" bindtap="tapDecCart">
                  <image src="{{api}}res/xcx/images/{{color}}icon_dec.png"></image>
                </view>
              </view>
            </view>
          </view>
          <view class="shch">
            <view>
              <text class="time" a:if="{{id.cookType!=''}}">[{{id.cookType}}]</text>
              <text a:if="{{id.cookMinute>0}}">{{id.cookMinute}}分钟</text>
            </view>
            <view class="number" a:if="{{id.orderCnt>0}}">{{id.orderCnt}}点过</view>
          </view>
          <view class="foodspec" a:for="{{id.sns}}" a:for-item="sns" a:key="unique">
            <text class="spec">{{sns.title}} </text>
